<div class="track track_playing">
	<img src="<%=track.urlImg%>">
        <p><%=track.name%></p>
        <audio src="<%=track.url%>" type="audio/mpeg" controls="controls"></audio>

        <form method="post" action="/tracks/<%=track.name%>?_method=delete">
        	<button type="submit" onClick="return confirm('Are you sure you want to delete <%=track.name%>? ');">
        		Delete track
        	</button>
        </form>
</div>
