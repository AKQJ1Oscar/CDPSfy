<div class="row">
    <div class="col-sm-12 text-center" style="margin-bottom: 25px;">
        <label style="font-size: 24px;">My tracks</label>
    </div>
</div>
<div class="photos">
    <%for (var t in tracks) {%>
    <ul class="col-xs-6 col-sm-6 col-md-4 col-lg-3">
        <li>
            <span>
                <a href='/tracks/<%=tracks[t].name%>' style="margin-bottom: 10px;">
                    <img src="<%=tracks[t].url_cover%>" style="width: 200px; border: 1px solid #333;">
                </a>
            </span>
            <span style="font-size: 16px; font-weight: 500; color: #333;">
                <%=tracks[t].name%>
            </span>
        </li>
    </ul>
    <%}%>
</div>
