<div class="row">
        <div class="col-sm-12 text-center" style="margin-bottom: 25px;">
                <label>My tracks</label>
        </div>
</div>
<div class="photos">
        <%for (var t in tracks) {%>
        
        <ul class="col-sm-3">
                <li>
                        <span>
                                <a href='/tracks/<%=tracks[t].name%>' style="margin-bottom: 10px;">
                                        <img src="<%=tracks[t].urlImg%>" style="width: 200px;">
                                </a>
                        </span>
                        <span>
                                <%=tracks[t].name%>
                        </span>
                </li>
        </ul>
        
        <%}%>
</div>
